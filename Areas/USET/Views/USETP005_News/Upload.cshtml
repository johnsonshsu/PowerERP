@{
    ViewData["Title"] = "上傳圖片";
    Layout = "_LayoutAdmin";
}

<form method="post" enctype="multipart/form-data">
    <div class="row">
        <div class="col-md-10">
            <input type="file" id="file" name="file" class="form-control" />
        </div>
        <div class="col-md-2">
            <button type="submit" class="btn btn-primary">上傳檔案</button>
        </div>
    </div>
</form>
<hr>
<div class="row">
    <div class="col-md-12">
        @Html.ActionLink("返回", ActionService.Index, ActionService.Controller, new { area = ActionService.Area }, new {
        @class = "btn btn-success" })
    </div>
</div>